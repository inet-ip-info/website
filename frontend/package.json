{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write \"**/*.{svelte,js,json,ts,md,html}\"",
    "lint": "eslint './src/**/*.{js,json,ts,svelte}'",
    "lint:fix": "eslint --fix './src/**/*.{js,json,ts,svelte}'",
    "prelint": "npm run format"
  },
  "devDependencies": {
    "@mdi/js": "^6.9.96",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@smui/card": "^6.0.0-beta.16",
    "@smui/data-table": "^6.0.0-beta.16",
    "@smui/icon-button": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@smui/top-app-bar": "^6.0.0-beta.16",
    "@tsconfig/svelte": "^3.0.0",
    "@types/netmask": "^1.0.30",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "ip-subnet-calculator": "^1.1.8",
    "mdi-svelte": "^1.1.2",
    "netmask": "^2.0.2",
    "eslint": "^8.15.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.77.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^2.0.2",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-material-ui": "^6.0.0-beta.16",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
